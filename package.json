{
  "name": "sonderswap",
  "private": true,
  "version": "0.0.0",
  "_comment": "Trigger Vercel deployment",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
